        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:paddingBottom="@dimen/margin15"
            android:paddingLeft="14dp"
            android:paddingTop="@dimen/margin20" >

            <TextView
                android:id="@+id/tv_withdraw_commission_count"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:text="0.00"
                android:textColor="@color/pink_color"
                android:textSize="30sp" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_withdraw_commission_count"
                android:background="@color/white" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" 
                    android:layout_centerVertical="true"
                    >

                    <TextView
                        android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="已提现"
                        android:textColor="@color/text1_color"
                        android:textSize="@dimen/tv_size14" />

                    <TextView
                         android:layout_gravity="center_vertical"
                        android:id="@+id/tv_already_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0.00"
                        android:textColor="@color/pink_color"
                        android:textSize="@dimen/tv_size14" />

                    <TextView
                         android:layout_gravity="center_vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="元"
                        android:textColor="@color/text1_color"
                        android:textSize="@dimen/tv_size14" />
                </LinearLayout>

                <Button
                    android:id="@+id/bt_alliance_withdraw"
                    android:layout_width="80dp"
                    android:layout_height="30dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="23dp"
                    android:background="@drawable/bg_square_choice_btn_withdraw"
                  android:layout_centerVertical="true"
                    android:text="提现"
                    android:textColor="@color/pink_color"
                    android:textSize="16sp" />
            </RelativeLayout>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.3dp"
            android:background="@color/line_color" />

        <View
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:background="#f4f4f4" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.3dp"
            android:background="@color/line_color" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:paddingBottom="39dp" >

            <LinearLayout
                android:id="@+id/ll_biz_alliance_revenue_income"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="39dp"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="累计收入(元)"
                    android:textColor="@color/text1_color"
                    android:textSize="@dimen/tv_size14" />

                <TextView
                    android:id="@+id/tv_total_income"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin15"
                    android:text="0.00"
                    android:textColor="@color/pink_color"
                    android:textSize="@dimen/tv_size14" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.3dp"
                android:layout_below="@id/ll_biz_alliance_revenue_income"
                android:layout_marginLeft="60dp"
                android:layout_marginRight="60dp"
                android:layout_marginTop="39dp"
                android:background="@drawable/line" />

            <View
                android:layout_width="1dp"
                android:layout_height="160dp"
                android:layout_centerInParent="true"
                android:layout_marginBottom="@dimen/margin10"
                android:layout_marginTop="@dimen/margin10"
                android:background="#f4f4f4" />

            <LinearLayout
                android:id="@+id/ll_biz_alliance_revenue_income_week"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/ll_biz_alliance_revenue_income"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="78dp"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="近一周收入(元)"
                    android:textColor="@color/text1_color"
                    android:textSize="@dimen/tv_size14" />

                <TextView
                    android:id="@+id/tv_week_income"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin15"
                    android:text="0.00"
                    android:textColor="@color/pink_color"
                    android:textSize="@dimen/tv_size14" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_biz_alliance_revenue_income_today"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="65dp"
                android:layout_marginTop="39dp"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="今日收入(元)"
                    android:textColor="@color/text1_color"
                    android:textSize="@dimen/tv_size14" />

                <TextView
                    android:id="@+id/tv_today_income"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin15"
                    android:text="0.00"
                    android:textColor="@color/pink_color"
                    android:textSize="@dimen/tv_size14" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_biz_alliance_revenue_income_month"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@id/ll_biz_alliance_revenue_income_today"
                android:layout_marginRight="50dp"
                android:layout_marginTop="78dp"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="近一月收入(元)"
                    android:textColor="@color/text1_color"
                    android:textSize="@dimen/tv_size14" />

                <TextView
                    android:id="@+id/tv_month_income"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin15"
                    android:text="0.00"
                    android:textColor="@color/pink_color"
                    android:textSize="@dimen/tv_size14" />
            </LinearLayout>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.3dp"
            android:background="@color/line_color" />
    </LinearLayout>

</ScrollView>